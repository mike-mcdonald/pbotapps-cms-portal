type Contact {
  id: Int!
  uuid: String!
  name: String!
  type: [String]
  phoneNumbers: [String]
  emailAddresses: [String]
}

input ContactInput {
  name: String!
  type: [String]
  phoneNumbers: [String]
  emailAddresses: [String]
}

type ContactResponse implements Response {
  errors: [Violation]
  contact: Contact
}

type ContactConnection {
  total: Int!
  items: [Contact!]
}
