{#
/**
 * @file
 * Theme override for vertical tabs.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The rendered tabs.
 *
 * @see template_preprocess_vertical_tabs()
 */
#}
{% set tabs_classes = tabs_classes|default([
]) %}
<div{{attributes.setAttribute('data-vertical-tabs-panes',TRUE).addClass(tabs_classes)}}>{{ children }}</div>
